{
    "name": "outscale-agent",
    "displayName": "Outscale Agent",
    "description": "An Outscale agent extension for VS Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "outscale-agent",
                    "title": "Outscale Agent",
                    "icon": "$(rocket)"
                }
            ]
        },
        "views": {
            "outscale-agent": [
                {
                    "id": "outscale-agent-view",
                    "name": "test",
                    "type": "webview"
                }
            ]
        },
        "commands": [
            {
                "command": "outscale-agent.openView",
                "title": "Open Outscale Agent",
                "category": "Outscale Agent"
            },
            {
                "command": "outscale-agent.createAgent",
                "title": "New Agent",
                "category": "Outscale Agent"
            },
            {
                "command": "outscale-agent.searchAgents",
                "title": "Search Agents",
                "category": "Outscale Agent"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "outscale-agent.createAgent",
                    "when": "view == outscale-agent-view",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "build": "./build.sh",
        "build:vsix": "npm run compile && npx vsce package"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "16.x",
        "@types/vscode": "^1.74.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vscode/test-electron": "^2.5.2",
        "@vscode/vsce": "^3.6.0",
        "eslint": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "publisher": "your-publisher-name",
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/vscode-extension.git"
    },
    "license": "MIT"
}